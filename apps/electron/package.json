{
  "name": "electron",
  "description": "Electron",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "packages/main/dist/index.js",
  "scripts": {
    "build": "pnpm run build:main && pnpm build:shared && pnpm run build:preload && pnpm run build:renderer",
    "build:shared": "cd ./packages/extension && vite build",
    "build:extension": "cd ./packages/extension && vite build",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "compile": "cross-env MODE=production pnpm run build && electron-builder build --config electron-builder.json --dir",
    "package": "electron-builder build --publish never",
    "dev": "node scripts/watch.js",
    "lint": "eslint . --ext js,mjs,cjs,ts,mts,cts,tsx",
    "typecheck:shared": "tsc --noEmit -p packages/shared/tsconfig.json",
    "typecheck:main": "tsc --noEmit -p packages/main/tsconfig.json",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "typecheck:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck:renderer": "tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "pnpm run typecheck:main && pnpm run typecheck:shared && pnpm run typecheck:preload && pnpm run typecheck:renderer",
    "postinstall": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/update-electron-vendors.js && electron-builder install-app-deps",
    "format": "npx prettier --write \"**/*.{js,mjs,cjs,ts,mts,cts,tsx,json}\"",
    "db:generate": "drizzle-kit generate --config \"./drizzle.config.ts\""
  },
  "devDependencies": {
    "@altdot/eslint-config": "workspace:*",
    "@altdot/extension": "workspace:*",
    "@altdot/native": "workspace:*",
    "@altdot/shared": "workspace:*",
    "@altdot/tailwind-config": "workspace:*",
    "@altdot/typescript-config": "workspace:*",
    "@altdot/ui": "workspace:*",
    "@electron-forge/cli": "^7.4.0",
    "@electron/rebuild": "^3.6.0",
    "@swc/core": "^1.6.6",
    "@types/adm-zip": "^0.5.5",
