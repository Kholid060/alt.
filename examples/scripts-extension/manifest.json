{
  "name": "script_extension",
  "author": "kholid060",
  "version": "0.0.1",
  "description": "script extension example",
  "title": "Script extensions",
  "icon": "icon:Code2",
  "extensionType": "default",
  "categories": ["Scripts"],
  "commands": [
    {
      "icon": "icon:Terminal",
      "name": "bash.sh",
      "title": "Bash script",
	  "type": "script",
      "arguments": [
        {
          "name": "text",
		  "title": "Text",
          "placeholder": "Text here",
          "type": "input:text"
        },
        {
          "name": "number-args",
		  "title": "Number",
          "placeholder": "Number here",
          "type": "input:number"
        },
        {
          "name": "select",
		  "title": "Select",
          "placeholder": "Select",
          "type": "select",
          "options": [
            {
              "label": "Option 1",
              "value": "1"
            },
            {
              "label": "Option 2",
              "value": "2"
            }
          ]
        }
      ]
    },
    {
      "name": "python.py",
      "title": "Python script",
	  "type": "script",
      "arguments": [
        {
          "name": "password",
		  "title": "Password?",
          "placeholder": "Password here",
          "type": "input:password"
        }
      ]
    },
    {
      "name": "javascript.js",
      "title": "JS script",
	  "type": "script",
      "arguments": [
        {
		  "title": "Password?",
          "name": "password",
          "placeholder": "Password here",
          "type": "input:password"
        }
      ]
    }
  ]
}